[
  {
    "name": "go-gen-ssl",
    "image": "dsalazar32/go-gen-ssl:latest",
    "cpu": 10,
    "memory": 100,
    "entrypoint": [ "go-gen-ssl" ],
    "environment": [
      {
        "name": "AWS_REGION",
        "value": "us-east-1"
      },
      {
        "name": "AWS_ECS_CLUSTER_NAME",
        "value": "Automata"
      }
    ],
    "command": [
      "generate",
      "-email", 
      "dsalazar@iomediums.com",
      "-d", 
      "iomediums.com",
      "-s3",
      "-auto-renew"
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/go-gen-ssl/logs",
        "awslogs-region": "us-east-1"
      }
    }
  }
]
